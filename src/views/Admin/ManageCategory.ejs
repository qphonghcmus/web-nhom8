<p id="lengthCategory" style="display:none"><%=listCategory.length%></p>
<%listCategory.forEach(function(val,i){%>
<div class="content-box-list-subscriber">
    <div class="content-box-list-subscriber-header p-sm-0 shadow-sm">
        <div class="content-box-title">
            <p><%=val%></p>
        </div>
                  
        <div class="content-box-tool">
            <a>
                <i class="fa fa-caret-down" aria-hidden="true"></i>
            </a>
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="content-box-list-subscriber-content shadow p-4 mb-4 bg-white">
        <div class="table-responsive">
            <% var path = "table-" + i%>
            <table class="table table-hover" id="<%=path%>">
               <thead>
                    <tr>
                        <th scope="col" class="th-sm">Mã bài viết</th>
                        <th scope="col">Tên bài viết</th>
                        <th scope="col">Trạng thái</th>
                        <th scope="col"></th>
                        <th scope="col"></th>
                    </tr>
                </thead>  
            </table>         
        </div>
    </div>
</div>

<%});%>

            
            
       
